{
  "name": "jauert/validator",
  "description": "Very lightweight validator builder.",
  "license": "MIT",
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.4"
  },
  "autoload": {
    "psr-4": {"Jauert\\": "src/Jauert"}
  },
  "autoload-dev": {
    "psr-4": {"Jauert\\": "tests/Jauert"}
  },
  "scripts": {
    "test": "php ./vendor/bin/phpunit"
  }
}
